{
  "name": "konzol",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.11.0",
  "description": "An unplugin plugin to add a powerful logging macro to JS/TS thats 0 bytes in production.",
  "license": "MIT",
  "homepage": "https://github.com/web-dev-sam/konsol",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/web-dev-sam/konsol.git"
  },
  "bugs": {
    "url": "https://github.com/web-dev-sam/konsol/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "logging",
    "console"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./astro": "./dist/astro.js",
    "./esbuild": "./dist/esbuild.js",
    "./farm": "./dist/farm.js",
    "./nuxt": "./dist/nuxt.js",
    "./rollup": "./dist/rollup.js",
    "./rspack": "./dist/rspack.js",
    "./types": "./dist/types.js",
    "./vite": "./dist/vite.js",
    "./webpack": "./dist/webpack.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "play": "vite",
    "dev": "tsdown -w",
    "build:virtual": "node --experimental-strip-types scripts/macros.ts",
    "build": "pnpm build:virtual && tsc -b && tsdown",
    "preview": "vite preview",
    "peggy": "peggy -o src/parser/parser.js --format es --dts src/parser/grammar.peggy",
    "test": "vitest"
  },
  "peerDependencies": {
    "esbuild": "*",
    "rollup": "^3",
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "@babel/generator": "^7.27.5",
    "@babel/parser": "^7.27.5",
    "@babel/traverse": "^7.27.4",
    "@babel/types": "^7.27.6",
    "magic-string": "^0.30.17",
    "unplugin": "^2.3.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.16.2",
    "@swc/core": "^1.12.7",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.20.7",
    "@types/node": "^22.15.29",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.30.1",
    "peggy": "^5.0.4",
    "tsdown": "^0.12.9",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  }
}
